#include<iostream>
using namespace std;
#include<iostream>
using namespace std;
void asterisks(int);

int main()
{
    int side;
    cout << "Please enter an integer for side: " << endl;
     cin >> side;
    asterisks (side) ;
    
}
void asterisks(int side)
    {
        for(int i=0;i<side;i++)
        {
            for(int t=0;t<side;t++)
            {
                cout << "*";
            }
            cout << endl;
        }
    }
